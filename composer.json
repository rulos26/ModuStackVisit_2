{
    "name": "miapp/php-structure",
    "description": "Estructura base para aplicaci√≥n PHP 8.3",
    "type": "project",
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "dompdf/dompdf": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Src\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
